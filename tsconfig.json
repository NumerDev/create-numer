{
  "compilerOptions": {
    "target": "ES2024",
    "module": "Preserve",
    "outDir": "dist",
    "moduleResolution": "bundler",
    "allowImportingTsExtensions": true,
    "strict": true,
    "skipLibCheck": true,
    "noUnusedLocals": true,
    "esModuleInterop": true,
    "noEmit": true,
    "noImplicitAny": true,
    "noUnusedParameters": true
  },
  "include": ["tsdown.config.ts", "src"]
}
